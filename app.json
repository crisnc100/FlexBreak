      "bundleIdentifier": "com.cristianortega.flexbreak",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.cristianortega.flexbreak"
    },
    "web": {},
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e2f2f0ca-229d-4469-9de8-9f69b7f7a724"
      }
    },
    "owner": "crisnc100"
  }
}
