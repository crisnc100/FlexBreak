{
  "expo": {
    "name": "FlexBreak",
    "slug": "flexbreak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/potentialLogo2.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cristianortega.flexbreak",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      },
      "splash": {
        "xib": "./ios/FlexBreak/SplashScreen.storyboard"
      }
    },
    "android": {
      "package": "com.cristianortega.flexbreak",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/potentialLogo2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "plugins": [
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/potentialLogo2.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e2f2f0ca-229d-4469-9de8-9f69b7f7a724"
      }
    },
    "owner": "crisnc100",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e2f2f0ca-229d-4469-9de8-9f69b7f7a724"
    },
    "scheme": "flexbreak-app",
    "jsEngine": "hermes"
  }
}
