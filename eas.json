{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "testflight": {
      "extends": "production",
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production",
        "EXPO_SKIP_SPLASH_SCREEN_GENERATION": "1"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {},
    "testflight": {
      "ios": {
        "appleId": "crisnc100@gmail.com",
        "ascAppId": "6728844881",
        "appleTeamId": "7LHNAAUJQ6"
      }
    }
  }
}
